<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontSizeSwitcherView">
        <attr name="scaleMin" format="float"/>
        <attr name="scaleMax" format="float"/>
        <attr name="scaleStep" format="float"/>
    </declare-styleable>

    <declare-styleable name="CollapsingTitleLayout">
        <attr name="expandedMargin" format="reference|dimension"/>
        <attr name="expandedMarginStart" format="reference|dimension"/>
        <attr name="expandedMarginBottom" format="reference|dimension"/>
        <attr name="expandedMarginEnd" format="reference|dimension"/>
        <attr name="expandedTextSize" format="reference|dimension"/>
        <attr name="collapsedTextSize" format="reference|dimension"/>
        <attr name="android:textAppearance"/>
        <attr name="textSizeInterpolator" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CollapsingTextAppearance">
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
    </declare-styleable>

    <declare-styleable name="DragFrameLayout">
        <attr name="shadowBackground" format="color"/>
        <attr name="deleteColor" format="color"/>
    </declare-styleable>

    <attr name="homePageHeaderBackground" format="reference|color"/>
    <attr name="globalButtonTextColor" format="reference|color"/>
    <attr name="globalButtonSelector" format="reference|color"/>
    <attr name="hexagonHeaderButtonColor" format="reference|color"/>
    <attr name="hexagonHeaderButtonPressedColor" format="reference|color"/>

    <attr name="addServiceColor" format="reference|color"/>
    <attr name="shadowBackgroundColor" format="reference|color"/>
    <attr name="dragDeleteColor" format="reference|color"/>

    <attr name="serviceInfoBackground" format="reference|color"/>

    <attr name="backgroundServiceItemThemedIcon" format="reference"/>

</resources>
